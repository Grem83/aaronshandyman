{
  "name": "probootstrap",
  "version": "1.0.0",
  "description": "The lighter and faster Bootstrap 4 subtheme for Drupal 8",
  "repository": "localhost",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=dev node ./node_modules/.bin/webpack",
    "prod": "node ./node_modules/.bin/webpack",
    "test": "node ./node_modules/.bin/webpack -d --display-modules",
    "start": "node ./node_modules/.bin/webpack-dev-server -hot --inline --progress",
    "build": "node ./node_modules/.bin/webpack -p --config webpack.config.js --hide-modules",
    "watch": "node ./node_modules/.bin/webpack --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "css-loader": "^0.28.7",
    "jquery": "^3.2.1",
    "node-sass": "^4.7.2",
    "popper.js": "^1.13.0",
    "sass": "^1.0.0-beta.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.9.0",
    "webpack-dev-server": "^2.9.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.1.2"
  }
}
